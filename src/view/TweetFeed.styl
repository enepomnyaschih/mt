.mt-tweet-feed
	background #fff
	border 1px solid rgba(0,0,0,0.45)
	border-radius 6px
	box-sizing border-box
	width 522px

	&-header
		color #333
		font-family Arial, sans-serif
		font-size 18px
		font-weight bold
		padding 10px
		text-shadow 0 1px 0 #fff

	&-footer
		border-top 1px solid #e8e8e8
		padding 8px
		text-align center
